    "version": "0.2.0",
    "configurations": [
            {
            "type": "lldb",
            "request": "launch",
            "name": "Debug 'rsa_crypt' tests",
            "cargo": {
                "args": [
                    "test"
                ]
            },
            "program": "${cargo:program}",
            "args": []
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rsa-crypt': PBKDF2 Key Derivation",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rsa-crypt",
                    "--package=rsa-crypt"
                ],
                "filter": {
                    "name": "rsa-crypt",
                    "kind": "bin"
                }
            },
            "args": [
                "password",
                "-a pbkdf2",
                "-h sha256",
                "-p test"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}
